
 <h2 class="page-header">Get Your Claim Status</h2>
{{!--<p>Welcome To My World</p>--}}
<div class="container">
  <h2>Patients Details</h2>
  <form>
    <div class="form-group">
      <label for="name">Patients Name</label>
      <input type="text" class="form-control" id="name" placeholder="Name Should Be Alphabet" name="name" value="">
    </div>
    <div class="form-group">
      <label for="claim">Claim ID</label>
      <input type="text" class="form-control" id="claim" placeholder="Claim ID should be a number" name="claim" value="">
    </div>
    <div class="form-group">
      <label for="mediassist">MediAssist ID</label>
      <input type="text" class="form-control" id="mediassist" placeholder="MediAssist ID should be a number" name="mediassist" value="">
    </div>
    <div class="form-group">
      <label for="date">Date of Admission</label>
      <input type="date" class="form-control" id="dateAdmission" placeholder="mm/dd/yyyy" name="dateAdmission" value="" datepicker-popup="dd/MM/yyyy" >
    </div>
    <div class="form-group">
      <label for="date">Date of Discharge</label>
      <input type="date" class="form-control" id="dateDischarge" placeholder="mm/dd/yyyy" name="dateDischarge" value="" datepicker-popup="dd/MM/yyyy" >
    </div>
    <div id="wrong" class="bg-danger"></div>
    <button type="button" class="btn btn-primary" onclick="myFunction()">View</button>
	<br/>
	<br/>
	<div>
		<table class="table table-hover">
    <thead>
      <tr>
        <th>Claim Id</th>
        <th>Claim Amount</th>
        <th>Hospital Name</th>
        <th>Beneficiary Details</th>
        <th>Claim Status</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><p id="claim id"></td>
        <td><p id="claim amount"></td>
        <td><p id="hospital name"></td>
        <td><p id="beneficiary details"></td>
        <td><p id="claim status"></td>
      </tr>
    </tbody>
  </table>
	</div>
  </form>
</div>
<script src="/js/app.js"></script>
